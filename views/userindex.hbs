<main class="index">

    <section class="container">
        <section class="user-container">
            {{#each users}}
                <div class="user">
                    <div class="user-img">
                        {{#if picture }}
                            <img src="/public/uploads/{{picture}}" alt="">
                        {{/if}}
                    </div>
                    <span>{{firstname}}</span>
                </div>
            {{/each}}
            
        </section>
    </section>
</main>
